type Test {
    id: ID!
    string: String
    bool: Boolean
    num: Int
}

type User {
    id: ID!
    email: String
    userName: String
    firstName: String
    lastName: String
}

type Query {
    tests: [Test]
    testsPagination(size: Int, page: Int): [Test]
    test(id: Int): Test
    users: [User]
    user(id: Int): User
}